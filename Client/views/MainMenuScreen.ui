<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Frame" id="MainMenuScreen">
    <property name="height">600</property>
    <property name="width">800</property>
    <layout manager="pack">
      <property name="anchor">center</property>
      <property name="expand">true</property>
      <property name="propagate">False</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="Container">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">center</property>
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="button_Group">
            <layout manager="pack">
              <property name="expand">true</property>
            </layout>
            <child>
              <object class="ttk.Button" id="Connect">
                <property name="command">Connect_Clicked</property>
                <property name="default">disabled</property>
                <property name="idtocommand">false</property>
                <property name="takefocus">false</property>
                <property name="text" translatable="yes">Connect</property>
                <property name="width">50</property>
                <layout manager="pack">
                  <property name="ipady">5</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="SetName">
                <property name="command">SetName_Clicked</property>
                <property name="idtocommand">false</property>
                <property name="takefocus">false</property>
                <property name="text" translatable="yes">Set Name</property>
                <property name="width">50</property>
                <layout manager="pack">
                  <property name="ipady">5</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame_1">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="expand">true</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Entry" id="Name">
                    <property name="textvariable">string:Name</property>
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="UsernameLabel">
                    <property name="text" translatable="yes">Username:</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="QuitGame">
                <property name="command">QuitGame_Clicked</property>
                <property name="idtocommand">false</property>
                <property name="takefocus">false</property>
                <property name="text" translatable="yes">Quit Game</property>
                <property name="width">50</property>
                <layout manager="pack">
                  <property name="ipady">5</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
