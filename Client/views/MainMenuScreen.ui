<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Frame" id="MainMenuScreen">
    <layout manager="pack">
      <property name="anchor">center</property>
      <property name="expand">true</property>
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="button_Group">
        <layout manager="grid">
          <property name="propagate">True</property>
          <property type="row" id="3" name="pad">0</property>
        </layout>
        <child>
          <object class="ttk.Button" id="Connect">
            <property name="command">Connect_Clicked</property>
            <property name="idtocommand">false</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Connect</property>
            <property name="width">50</property>
            <layout manager="pack">
              <property name="ipady">5</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="SetName">
            <property name="command">SetName_Clicked</property>
            <property name="idtocommand">false</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Set Name()</property>
            <property name="width">50</property>
            <layout manager="pack">
              <property name="ipady">5</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
