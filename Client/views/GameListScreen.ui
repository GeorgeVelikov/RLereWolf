<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Frame" id="GameListScreen">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="tk.Listbox" id="GetGamesList">
        <property name="activestyle">none</property>
        <property name="font">{Arial} 12 {}</property>
        <property name="selectforeground">#c8c8c8</property>
        <property name="setgrid">true</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="ButtonGroup">
        <property name="height">200</property>
        <property name="padx">25</property>
        <property name="pady">10</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Button" id="Disconnect">
            <property name="command">Disconnect_Clicked</property>
            <property name="idtocommand">false</property>
            <property name="text" translatable="yes">Disconnect</property>
            <property name="width">15</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property type="col" id="0" name="pad">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="Join">
            <property name="anchor">n</property>
            <property name="command">Join_Clicked</property>
            <property name="idtocommand">false</property>
            <property name="relief">raised</property>
            <property name="text" translatable="yes">Join</property>
            <property name="width">15</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property type="col" id="1" name="pad">0</property>
              <property type="col" id="1" name="uniform" />
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
