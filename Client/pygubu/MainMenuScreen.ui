<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Toplevel" id="root">
    <property name="geometry">800x600</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Client</property>
    <child>
      <object class="ttk.Frame" id="screen_MainMenu">
        <layout manager="pack">
          <property name="anchor">center</property>
          <property name="expand">true</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="button_Group">
            <layout manager="grid">
              <property name="propagate">True</property>
              <property type="row" id="3" name="pad">0</property>
            </layout>
            <child>
              <object class="ttk.Button" id="button_Connect">
                <property name="idtocommand">false</property>
                <property name="text" translatable="yes">Connect</property>
                <layout manager="pack">
                  <property name="ipadx">100</property>
                  <property name="ipady">5</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="button_SetName">
                <property name="default">normal</property>
                <property name="idtocommand">true</property>
                <property name="text" translatable="yes">Set Name</property>
                <layout manager="pack">
                  <property name="ipadx">100</property>
                  <property name="ipady">5</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
